<h1><%= @contact.first_name %> <%= @contact.last_name %></h1>
<h3>Email: <%= @contact.email %></h3>
<h4>Phone Number: <%= @contact.phone_number %></h4>
<h5><%= link_to "Return", "/contacts" %></h5>
<h5><%= link_to "Edit Contact", "/contacts/#{@contact.id}/edit" %></h5>
<h5><%= link_to "Delete", "/contacts/#{@contact.id}", method: :delete %></h5>